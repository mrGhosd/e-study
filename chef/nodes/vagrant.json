{
  "nginx": {
    "version": "2.7.6",
    "default_site_enabled": true,
    "source": {
      "modules": ["nginx::http_gzip_static_module", "nginx::http_ssl_module"]
    }
  },
  "rbenv": {
    "user_rubies": ["2.3.0"],
    "global": "2.3.0",
    "user_installs": [
      {
        "user": "vagrant",
        "global": "2.3.0",
        "rubies": ["2.3.0"],
        "gems": {
          "2.3.0": [
            { "name": "bundler" }
          ]
        }
      }
    ]
  },
  "run_list": [
    "recipe[nginx::package]",
    "recipe[ruby_build]",
    "recipe[ruby_rbenv::user]"
  ],
  "automatic": {
    "ipaddress": "127.0.0.1"
  }
}
